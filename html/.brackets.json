{
    "path": {
        "sass/main.scss": {
            "sass.enabled": true,
            "sass.options": {
                "outputDir": "../css/",
                "imagePath": null,
                "sourceComments": "map",
                "outputStyle": "nested"
            }
        },
        "sass/include/*.scss": {
            "sass.enabled": false,
            "linting.collapsed": false
        }
    }
}